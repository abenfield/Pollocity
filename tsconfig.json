{
  "compilerOptions": {
    "baseUrl": "./resources/js",
    "paths": {
      "components/*": ["components/*"],
      "shared/*": ["shared/*"],
      "routes/*": ["routes/*"],
      "utilities/*": ["utilities/*"]
    },
    "outDir": "./dist/",
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react-jsx",
    "allowJs": true,
    "moduleResolution": "node"
  },
  "include": [
    "./resources/js/*"
  ]
}